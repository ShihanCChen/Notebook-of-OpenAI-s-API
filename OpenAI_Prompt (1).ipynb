   "source": [
    "question = \"tell me a story about a econ phd student\"   ##Define the question Variable.\n",
    "print('ask:',question)      ##Print the Input Question.\n",
    "print('GPT:',send_request(question))  ##Generate Text Using the OpenAI API."
